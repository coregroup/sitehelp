// Descrição do programa: informar todos os numeros pares entra 100 e 200 
programa
{
	funcao inicio()
	{
		inteiro i
		para(i=100; i=<200; i++ ){ // A expressão (=<) está inversa
		se(i%2==0){
			escreva (i,"\n")
			}
		}
	}
}
