// Função : Escreva um algoritmo que imprima de 1 até 10.
// Autor : Methuzael Rodrigues
programa
{
	funcao inicio()
	{
		inteiro i

		para (i=1; i<=10; i++) {
			escreva (i ," ")			
	}
}
