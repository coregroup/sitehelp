// Função : Escreva um algoritmo que receba uma senha e compare se a senha é igual a "casa".
// Autor : Methuzael Rodrigues
programa
{
	funcao inicio()
	{
		cadeia senha
		inteiro cont
		
		escreva ("Informe a senha : ")
		leia (senha)
		cont = 1
		
		se (senha == "casa") escreva ("Bem vindo")
		senao escreva ("Senha Incorreta")
	}
}
